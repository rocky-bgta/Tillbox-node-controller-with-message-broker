{
  "name": "tillboxweb-node-back-end-with-message-broker",
  "version": "1.0.0",
  "Author": "Md. Nazmus Salahin Rocky",
  "description": "Key components of this project Express, Typescript, swagger, message broker",
  "license": "MIT",
  "scripts": {
    "run": "npm run build && npm run start",
    "start": "./node_modules/.bin/ts-node src/index.ts",
    "debug": "npm run build && npm run support_debug",
    "support_debug": "./node_modules/.bin/ts-node %NODE_DEBUG_OPTION% src/index.ts",
    "start:cluster": "./node_modules/.bin/ts-node src/cluster.ts",
    "build": "npm run clean && tsc && npm run swagger",
    "clean": "rimraf dist",
    "delete_Node_Module": "rimraf node_modules",
    "lint": "tslint ./src/**/*.ts ./test/**/*.ts",
    "format": "tsfmt -r",
    "swagger": "swaggerGen -c ./swagger.config.json",
    "tsc": "tsc && npm run swagger",
    "watch": "./node_modules/.bin/ts-node-dev dist/index.js"
  },
  "dependencies": {
    "@types/compression": "0.0.34",
    "@types/dotenv": "^4.0.2",
    "@types/helmet": "0.0.36",
    "@types/morgan": "^1.7.35",
    "@types/mqtt": "^2.5.0",
    "@types/winston": "^2.3.7",
    "class-transformer": "^0.1.8",
    "compression": "^1.7.1",
    "deasync": "^0.1.11",
    "dotenv": "^4.0.0",
    "helmet": "^3.9.0",
    "jsonwebtoken": "^8.1.0",
    "jsonwebtoken-refresh": "^5.7.0",
    "morgan": "^1.9.0",
    "mqtt": "^2.14.0",
    "reflect-metadata": "^0.1.10",
    "ts-node": "~3.0.6",
    "typescript-ioc": "^1.0.2",
    "typescript-rest": "^1.2.0",
    "typescript-rest-swagger": "0.0.10",
    "uniqid": "^4.1.1",
    "validator.ts": "^0.2.2",
    "winston": "^2.4.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.3",
    "cross-env": "^4.0.0",
    "gulp": "^3.9.1",
    "gulp-file-cache": "0.0.1",
    "gulp-nodemon": "^2.2.1",
    "gulp-rimraf": "^0.2.1",
    "gulp-run": "^1.7.1",
    "gulp-sequence": "^0.4.6",
    "gulp-task": "^2.1.0",
    "gulp-typescript": "^3.2.3",
    "rimraf": "^2.6.1",
    "source-map-support": "^0.4.15",
    "typedoc": "^0.5.10",
    "typescript": "~2.3.4",
    "typescript-formatter": "^5.2.0"
  }
}
